{
  "name": "ptemplate",
  "version": "0.1.0",
  "description": "A personal website template",
  "repository": "https://github.com/nethruster/ptemplate.git",
  "author": "Nethruster <support@nethruster.com>",
  "scripts": {
    "dev": "webpack-dev-server",
    "build": "webpack -p --mode production",
    "build-gz": "webpack -p --mode production --env.compress",
    "build-render": "rm -R dist ; npm run build && npm run pre-render",
    "pre-render": "react-snap"
  },
  "license": "MIT",
  "reactSnap": {
    "source": "dist",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "minifyHtml": {
      "sortClassName": false,
      "removeComments": true
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "ajax-promises": "^1.0.3",
    "glamor": "^2.20.40",
    "preact": "^8.2.5",
    "preact-compat": "^3.17.0",
    "preact-grecaptcha": "^0.2.1",
    "react-ink": "^6.2.0",
    "react-router-dom": "^4.1.2",
    "react-toastify": "^2.0.0-rc.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "autoprefixer": "^7.1.2",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "^0.28.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^2.0.6",
    "react-snap": "^1.23.0",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.19.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.1.11"
  },
  "main": "index.js"
}
